{
  "$schema": "./node_modules/@openapitools/openapi-generator-cli/config.schema.json",
  "spaces": 2,
  "generator-cli": {
    "version": "7.14.0",
    "generators": {
      "backend": {
        "generatorName": "typescript-fetch",
        "output": "#{cwd}/packages/api",
        "inputSpec": "http://localhost:5074/swagger/v1/swagger.json",
        "additionalProperties": {
          "npmName": "@mono/api",
          "supportsES6": "true",
          "withInterfaces": true,
          "npmVersion": "1.0.0"
        }
      }
    }
  }
}